              "transform": {
                "type": "formula",
                "expr": "datum[1]/datum[0]"
              }
            },
            "y": {
              "transform": {
                "type": "formula",
                "expr": "datum.slice(2)"
              }
            },
            "colors": {
              "attributePath": "line.color",
              "values": [
                "darkcyan",
                "darkslateblue",
                "darkgoldenrod",
                "crimson"
              ]
            },
            "config": {
              "mode": "lines",
              "type": "scatter",
              "line": {
                "width": 1,
                "color": "",
                "shape": "spline"
              }
            }
          },
