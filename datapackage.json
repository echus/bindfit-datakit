    {
      "name": "fitGraph",
      "spec": {
        "data": [
          {
            "name": "inputData",
            "x": {
              "transform": {
                "type": "formula",
                "expr": "datum[1]/datum[0]"
              }
            },
            "y": {
              "transform": {
                "type": "formula",
                "expr": "datum.slice(2)"
              }
            },
            "colors": {
              "attributePath": "marker.color",
              "values": [
                "darkcyan",
                "darkslateblue",
                "darkgoldenrod",
                "crimson"
              ]
            },
            "config": {
              "mode": "markers",
              "type": "scatter",
              "marker": {
                "size": 8,
                "color": "",
                "symbol": "circle-open-dot"
              }
            }
          },
          {
            "name": "outputFit",
