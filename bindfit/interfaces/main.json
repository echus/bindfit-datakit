{
  "name": "main",
  "gui": {
    "layout": {
      "rows": [
        {
          "name": "main-row",
          "style": "display: grid; gap: 1rem; grid-template-columns: repeat(4, minmax(0, 1fr));",
          "cells": [
            {
              "name": "control-panel",
              "style": "grid-column: span 1 / span 1; display: flex; flex-direction: column; gap: 0.5em;",
              "layout": {
                "rows": [
                  {
                    "cells": [
                      {
                        "name": "control-panel-data",
                        "variable": "data"
                      }
                    ]
                  },
                  {
                    "cells": [
                      {
                        "name": "control-panel-model",
                        "variable": "model"
                      }
                    ]
                  },
                  {
                    "cells": [
                      {
                        "name": "control-panel-parameters",
                        "resource": "inputParams",
                        "options": {
                          "size": "compact"
                        }
                      }
                    ]
                  },
                  {
                    "cells": [
                      {
                        "name": "control-panel-method",
                        "variable": "method"
                      }
                    ]
                  },
                  {
                    "cells": [
                      {
                        "name": "control-panel-flavour",
                        "variable": "flavour"
                      }
                    ]
                  },
                  {
                    "cells": [
                      {
                        "name": "control-panel-subinit",
                        "variable": "subInitValues"
                      }
                    ]
                  },
                  {
                    "cells": [
                      {
                        "name": "control-panel-dilution",
                        "variable": "dilutionCorrection"
                      }
                    ]
                  },
                  {
                    "cells": [
                      {
                        "name": "control-panel-execute",
                        "component": "execute"
                      }
                    ]
                  }
                ]
              }
            },
            {
              "name": "main-panel",
              "style": "grid-column: span 3 / span 3;",
              "layout": {
                "rows": [
                  {
                    "cells": [
                      {
                        "name": "main-panel-fit-summary",
                        "resource": "summary"
                      }
                    ]
                  },
                  {
                    "cells": [
                      {
                        "name": "main-panel-fit-params",
                        "resource": "outputParams"
                      }
                    ]
                  },
                  {
                    "cells": [
                      {
                        "name": "main-panel-fit-graph",
                        "view": "fitGraphHighcharts"
                      }
                    ]
                  },
                  {
                    "cells": [
                      {
                        "name": "main-panel-molefrac-graph",
                        "view": "molefracGraphHighcharts"
                      }
                    ]
                  }
                ]
              }
            }
          ]
        }
      ]
    }
  }
}
